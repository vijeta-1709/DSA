class Solution:
    def reverseString(self, s: List[str], left=0, right=None) -> None:
        if right is None:
            right = len(s) - 1   # Initialize on first call

    # Base case (recursion break point)
        if left >= right:
            return

    # Swap the elements
        s[left], s[right] = s[right], s[left]

    # Recursive call moving toward the center
        self.reverseString(s, left + 1, right - 1)

        
